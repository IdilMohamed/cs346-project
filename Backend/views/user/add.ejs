<%- contentFor('body') %>
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/admin">المستخدمين</a></li>
        <li class="breadcrumb-item active" aria-current="page">اضافة مستخدم</li>
    </ol>
</nav>
<div class="card">
    <div class="card-header">
        <h2  class="text-center">
            اضافة مستخدم
        </h2>
    </div>
    <div class="card-body">
        <form action="/signup" method="POST" enctype="multipart/form-data">
            <div class="mb-3 row">
                <label for="name" class="col-sm-2 col-form-label">اسم المستخدم</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="name" name="username">
                </div>
            </div>

            <div class="mb-3 row">
                <label for="universityName" class="col-sm-2 col-form-label"> البريد </label>
                <div class="col-sm-10">
                    <input type="email" class="form-control" id="universityName" name="email">
                </div>
            </div>
            <div class="mb-3 row">
                <label for="dateOfEvent" class="col-sm-2 col-form-label">كلمة السر </label>
                <div class="col-sm-10">
                    <input type="password" class="form-control" id="dateOfEvent" name="password">
                </div>
            </div>


            <button type="submit" class="btn btn-success w-100">
                حفــــظ
            </button>
        </form>
    </div>
</div>


 

     




