<%- contentFor('body') %>
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item active" aria-current="page">الدورات</li>
    </ol>
</nav>
<div class="card">
    <div class="card-header">
        <a href="/add-course" class="btn btn-info">
            اضافة دورة
        </a>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-sm table-bordered border-dark table-hover">
                <thead>
                <tr>
                    <th>#</th>
                    <th>اسم الدورة</th>
                    <th>اسم الجامعة</th>
                    <th>تاريخ الدورة</th>
                    <th>الموقع</th>
                    <th>العمليات</th>
                </tr>
                </thead>
                <tbody class="table-group-divider">
                <% theData.forEach((item, index)=>{ %>
                    <tr>
                        <td><%= index + 1 %></td>
                        <td><%= item.name %></td>
                        <td><%= item.universityName %></td>
                        <td><%= item.dateOfCourse %></td>
                        <td><%= item.courseLocation %></td>
                        <td>
                            <a class="btn btn-sm btn-danger" href="/delete-course/<%= item.id %>">
حذف
                            </a>

                        </td>
                    </tr>
                <% }) %>
                </tbody>

            </table>
        </div>
    </div>
</div>


 

     




